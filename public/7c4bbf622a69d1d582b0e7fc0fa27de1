的拦截函数\n      // 函数结果是一个promise，当resolve的时候提交结果\n      // reject的时候不提交结果\n      prevention: {\n        type: Function,\n        default: () => {}\n      },\n      tabTitle: {\n        type: String,\n        default: ''\n      }\n    },\n\n    data () {\n      return {\n        tabs: [],\n        children: null,\n        activeTab: null,\n        currentName: 0,\n        barStyle: ''\n      }\n    },\n\n    watch: {\n      activeName: {\n        handler (val) {\n          var fisrtKey = this.$children[0] && this.$children[0].key || '1'\n          this.currentName = val || fisrtKey\n        }\n      },\n\n      'currentName' () {\n        this.calcBarStyle()\n      }\n    },\n\n    methods: {\n      handleTabRemove (tab, ev) {\n        ev.stopPropagation()\n        tab.$destroy(true)\n\n        var index = this.tabs.indexOf(tab)\n\n        if (index !== -1) {\n          this.tabs.splice(index, 1)\n        }\n\n        if (tab.key === this.currentName) {\n          let deleteIndex = this.$children.indexOf(tab)\n          let nextChild = this.$children[deleteIndex + 1]\n          let prevChild = this.$children[deleteIndex - 1]\n\n          this.currentName = nextChild ? nextChild.key : prevChild ? prevChild.key : '-1'\n        }\n        this.$emit('tab-remove', tab.key)\n      },\n      handleTabClick (tab, event) {\n        let result = this.prevention(tab.key, event) || {}\n        ;(result.then ? result : Promise.resolve())\n          .then(() => {\n            this.currentName = tab.key\n            this.$emit('tab-click', tab.key, event)\n          })\n          .catch(() => {})\n      },\n      calcBarStyle (firstRendering) {\n        if (this.type || !this.$refs.tabs) return {}\n        var style = {}\n        var offset = this.tabTitle ? 220 : 0\n        var tabWidth = 0\n\n        this.tabs.every((tab, index) => {\n          let $el = this.$refs.tabs[index].$el\n          if (tab.key !== this.currentName) {\n            offset += $el.clientWidth\n            return true\n          } else {\n            tabWidth = $el.clientWidth\n            return false\n          }\n        })\n\n        style.width = tabWidth + 'px'\n        style.transform = `translateX(${offset}px)`\n\n        if (!firstRendering) {\n          style.transition = 'transform .3s cubic-bezier(.645,.045,.355,1), -webkit-transform .3s cubic-bezier(.645,.045,.355,1)'\n        }\n        this.barStyle = style\n      }\n    },\n    mounted () {\n      if (!this.currentName) {\n        var fisrtKey = this.$children[0] && this.$children[0].key || '1'\n        this.currentName = this.activeName || fisrtKey\n      }\n      this.$children.forEach(tab => {\n        if (tab.needShow) {\n          this.tabs.push(tab)\n        }\n      })\n      this.$nextTick(() => this.calcBarStyle(true))\n    }\n  }\n</script>\n<style>\n.x-tabs--card .x-tabs__item.is-active.is-closable .x-icon-close,.x-tabs--card .x-tabs__item.is-closable:hover .x-icon-close {\n  width: 14px\n}\n\n.x-tabs__header:after,.x-tabs__header:before {\n  display: table;\n  content: \"\"\n}\n\n.x-tabs__header:after {\n  clear: both\n}\n\n.x-tabs {\n  display: inline-block\n}\n\n.x-tabs__header {\n  border-bottom: 1px solid #d3dce6;\n  padding: 0;\n  position: relative;\n  margin: 0 0 15px\n}\n.x-tabs__title {\n  padding: 0 80px 0 40px;\n  height: 56px;\n  font-size: 18px;\n  color: #475669;\n  float: left;\n  line-height: 56px;\n  width: 100px;\n}\n.x-tabs__active-bar {\n  position: absolute;\n  bottom: -1px;\n  left: 0;\n  height: 3px;\n  background-color: #20a0ff;\n  z-index: 1;\n  list-style: none\n}\n\n.x-tabs__item {\n  padding: 0 16px;\n  height: 42px;\n  box-sizing: border-box;\n  line-height: 42px;\n  float: left;\n  list-style: none;\n  font-size: 14px;\n  color: #8492a6;\n  margin-bottom: -1px;\n  position: relative\n}\n\n.x-tabs__item:hover {\n  color: #1f2d3d;\n  cursor: pointer\n}\n\n.x-tabs__item.is-active {\n  color: #20a0ff\n}\n\n.x-tabs__content {\n  white-space: nowrap;\n  overflow: hidde