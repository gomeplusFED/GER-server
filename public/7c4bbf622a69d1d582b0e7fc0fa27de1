label\"\n        type=\"radio\"\n        v-model=\"model\"\n        @focus=\"focus = true\"\n        @blur=\"focus = false\"\n        :name=\"name\"\n        :disabled=\"disabled\">\n    </span>\n    <span class=\"x-radio__label\">\n      <slot></slot>\n      <template v-if=\"!$slots.default\">{{label}}</template>\n    </span>\n  </label>\n</template>\n<script>\n  import Emitter from 'components/mixins/emitter'\n  export default {\n    name: 'XRadio',\n    mixins: [Emitter],\n    componentName: 'XRadio',\n    props: {\n      value: {},\n      label: {},\n      disabled: Boolean,\n      name: String\n    },\n    data () {\n      return {\n        focus: false\n      }\n    },\n    computed: {\n      isGroup () {\n        let parent = this.$parent\n        while (parent) {\n          if (parent.$options.componentName !== 'XRadioGroup') {\n            parent = parent.$parent\n          } else {\n            this._radioGroup = parent\n            return true\n          }\n        }\n        return false\n      },\n      model: {\n        get () {\n          let flag = this.isGroup\n          let value = this._radioGroup.value\n          if (this._radioGroup.needArray) {\n            // 只给广告新建页 应用下载 appfeed中系统定向单选，左琳提的appid需求\n            value = value[0]\n          }\n          return flag ? value : this.value\n        },\n        set (val) {\n          if (this.isGroup) {\n            this._radioGroup.onchange(val)\n          } else {\n            this.$emit('input', val)\n          }\n        }\n      }\n    }\n  }\n</script>\n<style>\n.x-radio-button__inner,.x-radio__input {\n  line-height: 1;\n  vertical-align: middle;\n  outline: 0\n}\n\n.x-radio,.x-radio__inner,.x-radio__input {\n  position: relative;\n  cursor: pointer\n}\n\n.x-radio-button__inner,.x-radio__inner {\n  border: 1px solid #C0CCDA;\n  box-sizing: border-box\n}\n\n.x-radio,.x-radio-button__inner {\n  display: inline-block;\n  white-space: nowrap;\n  -moz-user-select: none;\n  -webkit-user-select: none