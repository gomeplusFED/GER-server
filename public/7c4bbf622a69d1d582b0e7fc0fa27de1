   :showAreaType=\"showAreaType\"\n        @tags=\"tagsHandler\"\n        @selectComplete=\"selectCompleteHandler\"\n        @areaTypeSelect=\"areaTypeHandler\">\n      </x-orientation-lbs>\n\n      <x-orientationother v-if=\"orientationType === 'rests'\"\n        v-clickoutside=\"clickoutsideHandler\"\n        v-show=\"show\"\n        v-model=\"initFlag\"\n        :datasource=\"datasource\"\n        :values=\"value\"\n        @tags=\"tagsHandler\"\n        @selectComplete=\"selectCompleteHandler\">\n      </x-orientationother>\n\n      <x-orientation-behavior v-if=\"orientationType === 'behavior'\"\n        v-clickoutside=\"clickoutsideHandler\"\n        v-show=\"show\"\n        v-model=\"initFlag\"\n        :datasource=\"datasource\"\n        :values=\"value.list\"\n        :chooseType=\"chooseType\"\n        @tags=\"tagsHandler\"\n        @selectComplete=\"selectBehaviorHandler\">\n      </x-orientation-behavior>\n    </transition>\n  </div>\n</template>\n\n\n<script>\n  // baseComponents\n  import Clickoutside from 'components/utils/clickoutside'\n  import XButton from 'components/button'\n  import XTag from 'components/tag'\n\n  // bizComponents\n  import XOrientationother from '../src/OrientationOthers'\n  import XOrientationarea from '../src/OrientationArea'\n  import XOrientationBehavior from '../src/OrientationBehavior'\n  import XOrientationLbs from '../src/OrientationLbs'\n\n  // others\n  import Emitter from 'components/mixins/emitter'\n  // import {getBDListByIdList} from 'api/Advert'\n\n  export default {\n    mixins: [Emitter],\n    props: {\n      datasource: {\n        type: [Object, Array]\n      },\n      value: {\n        type: [Array, Object]\n      },\n      orientationType: {\n        type: String,\n        default: 'area'\n      },\n      isinit: {\n        type: Boolean,\n        default: false\n      },\n      areaType: {\n        type: String\n      },\n      showAreaType: {\n        type: Boolean,\n        default: false\n      }\n    },\n    computed: {\n      tags () {\n        let tags = []\n        if (this.tagList) {\n          this.tagList.forEach(id => {\n            if (this.orientationType === 'area' && this.areaType === 'lbs') {\n              tags.push(id)\n            } else {\n              let city = this._getCityDataById(id)\n              if (city) {\n                tags.push({ key: city.id, name: city.name, type: 'gray' })\n              }\n            }\n          })\n        }\n        return tags\n      }\n    },\n    watch: {\n      value (val) {\n        if (this.orientationType === 'behavior') {\n          this.onSelectList = val.list\n          this.chooseType = val.type\n        } else {\n          this.onSelectList = val\n        }\n      },\n      onSelectList (value) {\n        if (this.orientationType === 'behavior') {\n          this.$emit('input', {type: this.chooseType, list: value})\n          this.dispatch('form-item', 'x.form.change', {type: this.chooseType, list: value})\n        } else {\n          this.$emit('input', value)\n          this.dispatch('form-item', 'x.form.change', value)\n        }\n      },\n      initFlag () {\n        // console.log('initFlag value: ' + this.initFlag)\n        this.$emit('init', this.initFlag)\n      },\n      isinit () {\n        this.initFlag = this.isinit\n      }\n    },\n    methods: {\n      handleClose (tag) {\n        this.tags.splice(this.tags.findIndex(item => tag.key === item.key), 1)\n        this.onSelectList.splice(this.onSelectList.indexOf(tag.key), 1)\n        if (this.orientationType === 'area' && this.areaType === 'lbs') {\n          this.tagList.splice(this.tagList.findIndex(item => tag.id === item.id), 1)\n        } else {\n          this._cancelSelectedByPid(this._getCityDataById(tag.key))\n        }\n      },\n      buttonClickHandler () {\n        this.show = !this.show\n        this.buttonText = this.show ? '确认选择' : '重新选择'\n      },\n      selectCompleteHandler (e) {\n        this.onSelectList = e\n      },\n      areaTypeHandler (e) {\n        this.onSelectList = e.list\n        this.show = true\n        this.$emit('areaTypeChange', e)\n      },\n      selectBehaviorHandler (e) {\n        this.chooseType = e.type\n        this.onSelectList = e.list\n      },\n      clickoutsideHandler () {\n        this.show = false\n        this.buttonText = '重新选择'\n      },\n      tagsHandler (e) {\n        this.tagList = e\n      },\n      /**\n       * [_cancelSelectedByPid 删除省下面的子集，并且将数据源设置为不选中状态]\n       * @param  {[type]} node [description]\n       * @return {[type]}      [description]\n       */\n      _cancelSelectedByPid (node) {\n        let children\n        let index = -1\n        node.isChecked = false\n        if (node.children) {\n          for (let i = 0; i < node.children.length; i++) {\n            children = node.children[i]\n            index = this.onSelectList.indexOf(children.id)\n            if (index !== -1) {\n              children.isChecked = false\n              this.onSelectList.splice(index, 1)\n            }\n            this._cancelSelectedByPid(children)\n          }\n        }\n      },\n      /**\n       * [_getCityDataById 通过id找一个城市，循环里面调用递归方法，挂了，日.......]\n       * @param  {[type]} id [城市ID]\n       * @return {[type]}    [description]\n       */\n      _getCityDataById (id) {\n        id = parseInt(id)\n        let tree = this.datasource\n\n        if (tree) {\n          if (tree.id === id) {\n            return tree\n          }\n          if (tree.children) {\n            let pdata\n            for (let i = 0, len = tree.children.length; i < len; i++) {\n              pdata = tree.children[i]\n              if (pdata.id === id) {\n                return pdata\n              } else {\n                if (pdata.children) {\n                  let cdata\n                  for (let j = 0, len = pdata.children.length; j < len; j++) {\n                    cdata = pdata.children[j]\n                    if (cdata.id === id) {\n                      return cdata\n                    }\n                  }\n                }\n              }\n            }\n          }\n        }\n        return null\n      }\n    },\n    created () {\n      if (this.areaType === 'lbs') {\n        this.show = true\n        this.buttonText = '确认选择'\n      }\n    },\n    mounted () {\n      // 配合clickoutside设置\n      this.$parent.popperElm = this.popperElm = this.$el\n    },\n    directives: {\n      Clickoutside\n    },\n    components: {\n      Clickoutside,\n      XButton,\n      XTag,\n      XOrientationarea,\n      XOrientationother,\n      XOrientationBehavior,\n      XOrientationLbs\n    },\n    data () {\n      return {\n        show: false,\n        buttonText: '重新选择',\n        onSelectList: this.value,\n        tagList: [],\n        initFlag: false,\n        chooseType: 'in'\n      }\n    }\n  }\n</script>\n\n\n<style scoped>\n  .confirmBtn{\n    background: #6fb6f1;\n    color: #fff;\n    float: right;\n    margin: 0 0 0 10px;\n  }\n  .x-tag{\n    margin: 0 3px 3px 0;\n  }\n  .orientation-box {\n    width: 100%;\n    margin: 5px 0 0 0;\n    min-height: 40px;\n  }\n  .verify-message {\n    color: #ff0000;\n    font-size: 14px;\n  }\n  .orientation-fade {\n    transition: all .3s ease;\n    height: 30px;\n    padding: 10px;\n    background-color: #eee;\n    overflow: hidden;\n  }\n  .choose-type {\n    font-size: 14px;\n    color: #475669;\n    margin-right: 10px;\n  }\n  .orientation-fade-enter-active {\n    -webkit-animation: orientation-fade-in .3s;\n    animation: orientation-fade-in .3s\n  }\n  .orientation-fade-leave-active {\n    -webkit-animation: orientation-fade-out .3s;\n    animation: orientation-fade-out .3s\n  }\n  @-webkit-keyframes orientation-fade-in {\n    0% {\n      -webkit-transform: translate3d(0,-20px,0);\n      transform: translate3d(0,-20px,0);\n      opacity: 0\n    }\n\n    100% {\n      -webkit-transform: translate3d(0,0,0);\n      transform: translate3d(0,0,0);\n      opacity: 1\n    }\n  }\n\