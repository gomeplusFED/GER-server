s: {\n      setCrowds () {\n        window.location.href = 'http://agent.ad.sina.com.cn/#/optimization/crowdpack'\n      },\n      crowdsChange (value) {\n        this.buttonShow = value\n        this.crowdsShow = value\n        // 此属性判断switch开关状态\n        this.crowdsDetail['disabled'] = !value\n        // if (!value) {\n          // 抛出新值\n        this.$emit('input', this.handleDetail(this.crowdsDetail))\n        // }\n      },\n      /**\n       * [handleAll] 处理全部按钮联动\n       * @return {[type]} [description]\n       */\n      handleAll () {\n        let _in = []\n        let _out = []\n        if ((Object.keys(this.crowdsDetail).length - 1) !== this.crowds.length) {\n          return\n        } else {\n          for (let key in this.crowdsDetail) {\n            let value = this.crowdsDetail[key]\n            switch (value) {\n              case `in|${key}`:\n                _in.push(value)\n                break\n              case `out|${key}`:\n                _out.push(value)\n                break\n              default:\n                break\n            }\n          }\n        }\n        if (_in.length === this.crowds.length) {\n          this.crowdsAll = 'allIn'\n        } else if (_out.length === this.crowds.length) {\n          this.crowdsAll = 'allOut'\n        } else {\n          this.crowdsAll = ''\n        }\n      },\n      handleChangeAll (value) {\n        if (!value) {\n          return\n        }\n        this.crowds.forEach((item) => {\n          if (value === 'allIn') {\n            this.crowdsDetail[item.cpId] = `in|${item.cpId}`\n          } else if (value === 'allOut') {\n            this.crowdsDetail[item.cpId] = `out|${item.cpId}`\n          }\n        })\n      },\n      handleDetail (detail) {\n        let _detail = {}\n        for (let key in detail) {\n          if (this.crowdsCheck[key] || key === 'disabled') {\n            _detail[key] = detail[key]\n          }\n        }\n        // this.isInit = false\n        return _detail\n      },\n      handleCheckAll (value) {\n        let e = window.event\n        if (e && e.target.name !== 'checkAll') {\n          this.checkAll = value\n          return\n        }\n        // checkbox自身触发事件处理\n        this.crowds.forEach(item => {\n          this.crowdsCheck[item.cpId] = value\n        })\n      }\n    },\n    created () {\n    },\n    computed: {\n      isInit () {\n        return this.isAdd\n      }\n      // crowdsState () {\n      //   return this.value ? !this.value.disabled : true\n      // }\n    },\n    watch: {\n      value (newValue, oldValue) {\n        // 默认值设置\n        this.crowdsState = !(newValue.disabled)\n        this.crowds.forEach(item => {\n          if (newValue[item.cpId]) {\n            this.crowdsDetail[item.cpId] = newValue[item.cpId]\n            this.crowdsCheck[item.cpId] = true\n          }\n        })\n      },\n      isInit: {\n        immediate: true,\n        handler (val) {\n          this.crowdsDetail.disabled = !val\n        }\n      },\n      crowds () {\n        // 初始化明细数据, 初始化checkbox\n        let crowdsDetail = {}\n        let crowdsCheck = {}\n        this.crowds.forEach((item) => {\n          crowdsDetail[item.cpId] = ''\n          crowdsCheck[item.cpId] = false\n        })\n        this.crowdsDetail = crowdsDetail\n        // disabled属性判断开关\n        if (!this.isInit) {\n          this.crowdsDetail.disabled = true\n        }\n        this.crowdsCheck = crowdsCheck\n      },\n      crowdsCheck: {\n        deep: true,\n        handler () {\n          let _all = true\n          this.crowds.forEach(item => {\n            if (!this.crowdsCheck[item.cpId]) {\n              _all = false\n            }\n          })\n          // 联动checkAll\n          this.checkAll = _all\n          // 抛出新值\n          this.$emit('input', this.handleDetail(this.crowdsDetail))\n        }\n      },\n      crowdsDetail: {\n        deep: true,\n        handler () {\n          // 抛出新值\n          this.$emit('input', this.handleDetail(this.c